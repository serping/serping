{
  "name": "serping",
  "version": "1.0.3",
  "description": "Serp.ing SDK",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx esbuild index.ts --bundle --platform=node --target=node20 --outdir=dist",
    "release": "npx standard-version --prerelease beta",
    "add-build": "npm run build",
    "test": "npx jest"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.js"
    },
    "./src/*": "./src/*"
  },
  "keywords": [
    "serp checker",
    "serp api",
    "serp scraper"
  ],
  "author": "Sead Feng",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "axios-mock-adapter": "^1.22.0",
    "esbuild": "^0.23.0",
    "jest": "^29.7.0",
    "pre-commit": "^1.2.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "pre-commit": [
    "add-build"
  ],
  "files": [
    "dist",
    "src",
    "tsconfig.json"
  ]
}
